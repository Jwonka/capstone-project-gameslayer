@page
@model VideoGameGrade.Pages.TestPageModel
@{
    ViewData["Title"] = "Test Database Connection";
}

<h1>@ViewData["Title"]</h1>

@if (Model.DatabaseConnected)
{
    <p style="color:green;">Database connection successful!</p>
}
else
{
    <p style="color:red;">Failed to connect to the database.</p>
}